<h1>All Shelters</h1>
<% @shelters.each do |shelter| %>
  <section id = <%="#{shelter.id}" %>>
    <h2><%= shelter.name %></h2>
    <%= link_to "Edit Shelter", "/shelters/#{shelter.id}/edit" %>
    <%= link_to "Delete Shelter", "/shelters/#{shelter.id}/delete" %>
  </section>
<% end %>
<br>
<br>
<%= link_to "New Shelter", "/shelters/new" %>
