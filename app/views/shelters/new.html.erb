<h1>New Shelter</h1>

<%= form_tag "new_shelter" do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  <%= label_tag :address %>
  <%= text_field_tag :address %>
  <%= label_tag :city %>
  <%= text_field_tag :city %>
  <%= label_tag :state %>
  <%= text_field_tag :state %>
  <%= label_tag :zip %>
  <%= text_field_tag :zip %>
  <%= submit_tag 'Create Shelter' %>
<% end %>

