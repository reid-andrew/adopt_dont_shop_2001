<nav>
  <%= link_to "All Pets", "/pets" %>
  <%= link_to "All Shelters", "/shelters" %>
</nav>
<content>
  <h1>New Shelter</h1>

  <%= form_tag "new_shelter" do %>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
    <br>
    <%= label_tag :address %>
    <%= text_field_tag :address %>
    <br>
    <%= label_tag :city %>
    <%= text_field_tag :city %>
    <br>
    <%= label_tag :state %>
    <%= text_field_tag :state %>
    <br>
    <%= label_tag :zip %>
    <%= text_field_tag :zip %>
    <br>
    <%= submit_tag 'Create Shelter' %>
  <% end %>
</content>
